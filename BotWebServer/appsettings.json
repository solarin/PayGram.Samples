{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "MyTelegramId": "xxxxxxxxxxxx",
  "BotToken": "xxxxxxxxxxxxxxxxxx",
  "TelegramCallback": "http://127.0.0.1/",
  "TelegramSecret": "telegramSecretToken", // the bot registers a callback that must contain this url, so we are sure that it was telegram to contact us
  "PayGramSecret": "payGramSecretToken" // paygram will callback using this token, so we are sure that it was paygram to contact us
}
